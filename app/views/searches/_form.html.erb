<% if user_signed_in? %>
　<div class='container'>
  　<div class='row'>
    　<div class="col-sm-12 col-lg-5 px-5 px-sm-0 mx-auto mt-1"
        <%= form_tag(search_path, method: :get) do %>
          <%= text_field_tag 'content' %>
          <%= select_tag 'model', options_for_select({ "User" => "user", "Book" => "book" }) %>
          <%= select_tag 'method', options_for_select({ "完全一致" => "perfect", "部分一致" => "partial" }) %>
          <%= submit_tag '検索' %>
        <% end %>
      </div>
    </div>
  </div>
<% end %>